{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
  
        {
            "name": "node.js debugging",
            "program": "${workspaceFolder}//${relativeFile}",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },

        
        {
            "type": "chrome",
            "request": "launch",
            "name": "barebone.html",
            "url": "http://localhost:5500/${relativeFileDirname}/../barbone.html",
            "webRoot": "${workspaceFolder}"
        },
         {
            "type": "chrome",
            "request": "launch",
            "name": "full.html",
            "url": "http://localhost:5500/${relativeFileDirname}/../full.html",
            "webRoot": "${workspaceFolder}"
        },

         {
            "type": "chrome",
            "request": "launch",
            "name": "mbands-homepage",
            "url": "http://localhost:5500/${relativeFileDirname}/../index.html",
            "webRoot": "${workspaceFolder}"
        },
             {
            "type": "chrome",
            "request": "launch",
            "name": "mbands-view",
            "url": "http://localhost:5500/${relativeFileDirname}/../mbands-view.html",
            "webRoot": "${workspaceFolder}"
        }

        {
            "type": "chrome",
            "request": "launch",
            "name": "CHAT",
            "url": "http://localhost:5500/${relativeFileDirname}/../chat.html",
            "webRoot": "${workspaceFolder}"
        }
    ]
}